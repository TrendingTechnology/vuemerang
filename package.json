{
  "name": "vuemerang",
  "version": "1.2.10",
  "description": "Framework Components for Vue js",
  "license": "MIT",
  "author": "Ivan Sotelo <isotelo@controlla.com.mx>",
  "homepage": "https://Controlla.github.io/vuemerang/#/",
  "repository": "controlla/vuemerang",
  "bugs": {
    "url": "https://github.com/controlla/vuemerang/issues"
  },
  "keywords": [
    "vuemerang",
    "vue",
    "vuejs",
    "vue-components",
    "components",
    "component",
    "javascript",
    "css",
    "framework",
    "ui",
    "frontend",
    "responsive",
    "stylus"
  ],
  "main": "dist/vuemerang.common.js",
  "unpkg": "dist/vuemerang.umd.min.js",
  "jsdelivr": "dist/vuemerang.umd.min.js",
  "private": false,
  "scripts": {
    "dev": "vuepress dev docs",
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build --target lib src/index.js --name vuemerang",
    "lint": "vue-cli-service lint",
    "v": "npm version patch",
    "v2": "npm version minor",
    "p": "npm publish",
    "deploy": "git commit -m 'deploy: Update Vuemerang' && npm run v && npm run p && git push",
    "deploy2": "npm run build && git add . & git commit -m 'deploy: Update Vuemerang' & npm run v2 & npm run p && git push",
    "docs:build": "vuepress build docs",
    "docs:deploy": "push-dir --dir=docs/.vuepress/dist --branch=gh-pages --cleanup",
    "deployx": "npm run docs:deploy && npm run deploy",
    "deployx2": "npm run docs:deploy && npm run deploy2",
    "new:component": "./scripts/newComponent.sh"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "debounce": "^1.2.0",
    "js-calendar": "^1.2.3",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "simple-jscalendar": "^1.4.3",
    "v-click-outside-x": "^3.5.6",
    "vue": "^2.5.17",
    "vue-loader": "^15.6.3",
    "vue2-dropzone": "^3.5.2",
    "vuelidate": "^0.7.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@vue/cli-plugin-babel": "^3.0.3",
    "@vue/cli-plugin-eslint": "^3.0.3",
    "@vue/cli-service": "^3.0.3",
    "copy-webpack-plugin": "^4.5.2",
    "eslint": "^5.14.1",
    "eslint-plugin-vue": "^4.7.1",
    "file-loader": "^1.1.11",
    "prefixfree": "^1.0.0",
    "push-dir": "^0.4.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "viewcode": "0.0.27",
    "vue-template-compiler": "^2.5.17",
    "vuepress": "^0.14.9",
    "webpack": "^4.29.5"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
